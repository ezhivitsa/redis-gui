{
  "author": "Evgeny Zhivitsa <evgeny.zhivitsa@gmail.com>",
  "name": "redis-gui",
  "version": "0.1.0",
  "description": "Graphical user interface for the Redis database",
  "main": "./src/main/main.ts",
  "scripts": {
    "postinstall": "make postinstall",
    "start": "make dev",
    "prepare": "husky install"
  },
  "build": {
    "productName": "Redis GUI",
    "appId": "org.ezhivitsa.RedisGUI",
    "asar": true,
    "asarUnpack": "**\\*.{node,dll}",
    "files": [
      "dist",
      "node_modules",
      "package.json"
    ],
    "afterSign": "configs/scripts/notarize.js",
    "mac": {
      "target": {
        "target": "default",
        "arch": [
          "arm64",
          "x64"
        ]
      },
      "type": "distribution",
      "hardenedRuntime": true,
      "entitlements": "assets/entitlements.mac.plist",
      "entitlementsInherit": "assets/entitlements.mac.plist",
      "gatekeeperAssess": false
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "app": "release/app",
      "buildResources": "assets",
      "output": "release/build"
    },
    "extraResources": [
      "./assets/**"
    ],
    "publish": {
      "provider": "github",
      "owner": "ezhivitsa",
      "repo": "redis-gui"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ezhivitsa/redis-gui.git"
  },
  "license": "MIT",
  "keywords": [
    "redis",
    "gui",
    "electron",
    "react",
    "typescript"
  ],
  "dependencies": {
    "@electron/remote": "2.0.9",
    "@fortawesome/fontawesome-svg-core": "6.3.0",
    "@fortawesome/free-solid-svg-icons": "6.3.0",
    "@fortawesome/react-fontawesome": "0.2.0",
    "classnames": "2.3.2",
    "electron-debug": "3.2.0",
    "electron-log": "4.4.8",
    "electron-updater": "5.3.0",
    "formik": "2.2.9",
    "history": "5.3.0",
    "idb": "7.1.1",
    "ioredis": "5.3.1",
    "lodash": "4.17.21",
    "mobx": "6.8.0",
    "mobx-react-lite": "3.4.0",
    "normalize.css": "8.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-modal": "3.16.1",
    "react-router-dom": "6.8.1",
    "ssh2": "1.11.0",
    "uuid": "9.0.0",
    "yup": "0.32.9"
  },
  "devDependencies": {
    "@electron/notarize": "1.2.3",
    "@electron/rebuild": "3.2.10",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.10",
    "@teamsupercell/typings-for-css-modules-loader": "2.5.2",
    "@types/history": "5.0.0",
    "@types/ioredis": "5.0.0",
    "@types/lodash": "4.14.191",
    "@types/node": "18.14.2",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/react-modal": "3.13.1",
    "@types/react-router-dom": "5.3.3",
    "@types/ssh2": "1.11.7",
    "@types/terser-webpack-plugin": "5.2.0",
    "@types/uuid": "9.0.1",
    "@types/webpack-bundle-analyzer": "4.6.0",
    "@types/yup": "0.32.0",
    "@typescript-eslint/eslint-plugin": "5.54.0",
    "@typescript-eslint/parser": "5.54.0",
    "chalk": "4.1.2",
    "core-js": "3.29.0",
    "cross-env": "7.0.3",
    "css-loader": "6.7.3",
    "css-minimizer-webpack-plugin": "4.2.2",
    "cssnano": "5.1.15",
    "detect-port": "1.5.1",
    "electron": "23.1.1",
    "electron-builder": "23.6.0",
    "electron-devtools-installer": "3.2.0",
    "electron-notarize": "1.1.0",
    "electron-rebuild": "3.2.2",
    "electronmon": "2.0.2",
    "eslint": "8.35.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "fork-ts-checker-webpack-plugin": "7.3.0",
    "html-webpack-plugin": "5.5.0",
    "husky": "8.0.3",
    "identity-obj-proxy": "3.0.0",
    "lint-staged": "13.1.2",
    "mini-css-extract-plugin": "2.7.2",
    "postcss": "8.3.6",
    "postcss-custom-properties": "11.0.0",
    "postcss-import": "14.0.2",
    "postcss-loader": "6.1.1",
    "postcss-nested": "5.0.6",
    "postcss-preset-env": "6.7.0",
    "prettier": "2.8.4",
    "react-refresh": "0.14.0",
    "rimraf": "4.1.2",
    "style-loader": "3.3.1",
    "stylelint": "15.2.0",
    "stylelint-config-prettier": "9.0.5",
    "stylelint-config-standard": "30.0.1",
    "terser-webpack-plugin": "5.3.6",
    "ts-loader": "9.4.2",
    "ts-node": "10.9.1",
    "tsc-files": "1.1.3",
    "tsconfig-paths-webpack-plugin": "4.0.0",
    "typescript": "4.9.5",
    "webpack": "5.75.0",
    "webpack-bundle-analyzer": "4.8.0",
    "webpack-cli": "5.0.1",
    "webpack-dev-server": "4.11.1",
    "webpack-merge": "5.8.0"
  },
  "devEngines": {
    "node": ">=14.x",
    "npm": ">=7.x"
  },
  "electronmon": {
    "patterns": [
      "!**/**",
      "src/main/**"
    ],
    "logLevel": "quiet"
  }
}
