    "@fortawesome/free-solid-svg-icons": "5.15.4",
    "@fortawesome/react-fontawesome": "0.1.15",
    "classnames": "2.3.1",
    "electron-debug": "3.2.0",
    "electron-log": "4.4.1",
    "electron-updater": "4.3.9",
    "formik": "2.2.9",
    "history": "5.0.1",
    "idb": "6.1.2",
    "lodash": "4.17.21",
    "mobx": "6.3.2",
